{
	"name": "entities",
	"version": "1.0.0",
	"description": "",
	"main": "dist/index.html",
	"scripts": {
		"gitlog": "./node_modules/.bin/git-json > git-log.json",
		"OH-PARCEL-COME-ON": "npm run gitlog && node ./lol.js",
		"start": "npm run OH-PARCEL-COME-ON && parcel ./src/index.html",
		"make": "npm run OH-PARCEL-COME-ON && parcel build ./src/index.html",
		"circles": "madge --circular --extensions=ts ./src"
	},
	"madge": {
		"tsConfig": "tsconfig.json",
		"detectiveOptions": {
			"ts": {
				"skipTypeImports": true
			}
		}
	},
	"staticFiles": {
		"staticPath": "static",
		"watcherGlob": "**"
	},
	"author": "walaura",
	"license": "ISC",
	"dependencies": {
		"@babel/preset-env": "^7.9.5",
		"@rollup/plugin-commonjs": "^11.1.0",
		"@rollup/plugin-html": "^0.1.1",
		"@rollup/plugin-multi-entry": "^3.0.0",
		"@rollup/plugin-node-resolve": "^7.1.3",
		"@rollup/plugin-replace": "^2.3.1",
		"@rollup/plugin-typescript": "^4.1.1",
		"@rollup/plugin-url": "^4.0.2",
		"@types/node": "^13.11.0",
		"deepmerge": "^4.2.2",
		"git2json": "0.0.1",
		"hash-sum": "^2.0.0",
		"line-replace": "^1.0.3",
		"madge": "^3.8.0",
		"nanoid": "^3.0.2",
		"parcel": "^2.0.0-alpha.3.2",
		"parcel-plugin-static-files-copy": "^2.3.1",
		"perlin-noise": "0.0.1",
		"postcss": "^7.0.27",
		"postcss-nested": "^4.2.1",
		"preact": "^10.4.0",
		"rollup": "^2.6.1",
		"rollup-plugin-babel": "^4.4.0",
		"rollup-plugin-serve": "^1.0.1",
		"shortid": "^2.2.15",
		"uniqid": "^5.2.0"
	},
	"browserslist": [
		"last 1 Chrome versions"
	],
	"engines": {
		"node": ">= 12"
	},
	"devDependencies": {
		"cssnano": "^4.1.10",
		"typescript": "^3.8.3"
	}
}
