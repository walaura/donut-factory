{
	"name": "entities",
	"version": "1.0.0",
	"description": "",
	"main": "dist/index.html",
	"scripts": {
		"gitlog": "./node_modules/.bin/git-json > git-log.json",
		"start": "node_modules/.bin/webpack-dev-server",
		"make": "npm run gitlog && cp -a ./static/. ./dist/ && webpack",
		"circles": "madge --circular --extensions=ts ./src"
	},
	"madge": {
		"tsConfig": "tsconfig.json",
		"detectiveOptions": {
			"ts": {
				"skipTypeImports": true
			}
		}
	},
	"staticFiles": {
		"staticPath": "static",
		"watcherGlob": "**"
	},
	"author": "walaura",
	"license": "ISC",
	"dependencies": {
		"@types/node": "^13.11.0",
		"deepmerge": "^4.2.2",
		"file-loader": "^6.0.0",
		"git2json": "0.0.1",
		"hash-sum": "^2.0.0",
		"html-webpack-plugin": "^4.2.0",
		"line-replace": "^1.0.3",
		"madge": "^3.8.0",
		"nanoid": "^3.0.2",
		"perlin-noise": "0.0.1",
		"postcss": "^7.0.27",
		"postcss-nested": "^4.2.1",
		"preact": "^10.4.0",
		"shortid": "^2.2.15",
		"ts-loader": "^7.0.0",
		"typescript": "^3.8.3",
		"uniqid": "^5.2.0",
		"webpack": "^4.42.1",
		"webpack-bundle-analyzer": "^3.7.0",
		"webpack-cli": "^3.3.11",
		"webpack-dev-server": "^4.0.0",
		"worker-loader": "^2.0.0"
	},
	"browserslist": [
		"last 1 Chrome versions"
	],
	"engines": {
		"node": ">= 12"
	}
